<!DOCTYPE html>
<html>
<head>
  <title>VikingsApp</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<% if notice.present? %>
	<p><%= notice %></p>
<% end %>

<% if alert.present? %>
	<p><%= alert %></p>
<% end %>

<%= yield %>

</body>
</html>
